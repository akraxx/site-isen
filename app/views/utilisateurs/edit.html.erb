<h1>Editing utilisateur</h1>

<% form_for(@utilisateur) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :login %><br />
    <%= f.text_field :login %>
  </p>
  <p>
    <%= f.label :pwd %><br />
    <%= f.text_field :pwd %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @utilisateur %> |
<%= link_to 'Back', utilisateurs_path %>
